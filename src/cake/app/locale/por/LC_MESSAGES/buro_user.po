msgid "This gallery"
msgstr "Sobre essa galeria"

#: \plugins\burocrata_user\views\exemplos\index.ctp:28
msgid "Gallery title"
msgstr "Título da galeria"

#: \plugins\burocrata_user\views\exemplos\index.ctp:35
msgid "Gallery release date"
msgstr "Data de publicação"

#: \plugins\burocrata_user\views\exemplos\index.ctp:50
msgid "Something about this gallery"
msgstr "Descrição da galeria"

#: \plugins\burocrata_user\views\exemplos\index.ctp:64
msgid "Owner of this gallery"
